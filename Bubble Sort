#bubble sort
def BubbleSort(arr):
    n = len(arr)
    for i in range(n):
        for j in range (0, n-i-1):
            if arr[j] > arr[j+1]:
                arr[j], arr[j+1] = arr[j+1], arr[j]
                
arr = [6, 9, 5, 2, 1, 4]
print(arr)
BubbleSort(arr)
print("Sorted Array: ")
for i in range (len(arr)):
    print("%d" %arr[i], end=" ")
