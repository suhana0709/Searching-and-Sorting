# optimized bubble sort
def BubbleSort(arr):
    n = len(arr)
    for i in range(n):
        swap = False
        for j in range(0, n-i-1):
            if arr[j] > arr[j+1]:
                arr[j], arr[j+1] = arr[j+1], arr[j]
                swap = True
        if swap == False:
            break
        
arr = [35, 2, 67, 45, 5, 0, 12]
print(arr)
BubbleSort(arr)
print("Sorted Array: ")
for i in range(len(arr)):
    print("%d" %arr[i], end=" ")
